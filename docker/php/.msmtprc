# Set default values for all following accounts.
defaults
auth off
#tls on
#tls_trust_file /etc/ssl/certs/ca-certificates.crt
syslog on
logfile ~/.msmtp.log
aliases /etc/aliases

# mailtrap
account mailhog
host smtp
port 1025
from info@example.com
user username
password password

# Set a default account
account default : mailhog